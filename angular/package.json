{
    "name": "django-angular-dynamic-forms",
    "version": "0.6.4",
    "license": "MIT",
    "description": "Django Rest Framework meets Angular 5 material.io dynamic forms - rapid development of create and edit dialogs",
    "typings": "./django-angular-dynamic-forms.d.ts",
    "main": "./bundles/django-angular-dynamic-forms.umd.js",
    "module": "./esm5/django-angular-dynamic-forms.js",
    "es2015": "./esm2015/django-angular-dynamic-forms.js",
    "readme": "README.rst",
    "keywords": [],
    "scripts": {
        "build": "npm run pack:library",
        "ci": "npm run lint && npm run test && npm run build && npm run docs",
        "codecov": "cat coverage/lcov.info | codecov",
        "docs": "compodoc -p tsconfig.json -d docs --disableCoverage",
        "explorer": "source-map-explorer ./dist//bundles/django-angular-dynamic-forms.umd.js",
        "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
        "lint": "npm run tslint 'src/**/*.ts'",
        "postversion": "git push && git push --tags",
        "prebuild": "rimraf dist tmp lib-dist",
        "preversion": "npm run ci",
        "test": "karma start",
        "test:watch": "karma start --auto-watch --no-single-run",
        "tslint": "tslint",
        "pack:library": "npm run prebuild && angular-package-builder && cp package-dist.json dist/package.json && copyfiles -u 1 ../README.rst ../LICENSE dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mesemus/django-angular-dynamic-forms.git"
    },
    "author": {
        "name": "Mirek Simek",
        "email": "miroslav.simek+github@gmail.com",
        "url": "https://github.com/mesemus/django-angular-dynamic-forms"
    },
    "peerDependencies": {
        "@angular/common": "^5.2.0",
        "@angular/core": "^5.2.0",
        "@angular/flex-layout": "^2.0.0-beta.12",
        "@angular/forms": "^5.2.0",
        "@angular/material": "^5.2.2",
        "@ng-dynamic-forms/core": "^5.4.4",
        "@ng-dynamic-forms/ui-material": "^5.4.4",
        "core-js": "^2.4.1",
        "rxjs": "^5.5.6"
    },
    "devDependencies": {
        "@angular/animations": "^5.2.0",
        "@angular/cdk": "^5.1.1",
        "@angular/cli": "^1.6.8",
        "@angular/common": "^5.2.0",
        "@angular/compiler": "^5.2.0",
        "@angular/compiler-cli": "^5.2.4",
        "@angular/core": "^5.2.0",
        "@angular/flex-layout": "^2.0.0-beta.12",
        "@angular/forms": "^5.2.0",
        "@angular/http": "^5.2.0",
        "@angular/material": "^5.1.1",
        "@angular/platform-browser": "^5.2.0",
        "@angular/platform-browser-dynamic": "^5.2.0",
        "@angular/router": "^5.2.0",
        "@compodoc/compodoc": "^1.0.0-beta.9",
        "@ng-dynamic-forms/core": "^5.4.4",
        "@ng-dynamic-forms/ui-material": "^5.4.4",
        "@types/jasmine": "^2.5.47",
        "@types/karma": "^1.7.3",
        "@types/node": "^9.4.5",
        "@types/webpack": "^3.0.13",
        "@types/webpack-env": "^1.13.0",
        "angular-package-builder": "^1.0.1",
        "angular2-highlight-js": "^6.0.1-alpha",
        "angular2-template-loader": "^0.6.2",
        "angular2-text-mask": "^8.0.4",
        "awesome-typescript-loader": "^3.4.1",
        "codecov": "^3.0.0",
        "codelyzer": "^4.1.0",
        "copyfiles": "^1.2.0",
        "jasmine-core": "^2.99.1",
        "karma": "^2.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage-istanbul-reporter": "^1.4.1",
        "karma-jasmine": "^1.1.1",
        "karma-mocha-reporter": "^2.2.5",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^2.0.9",
        "rxjs": "^5.5.6",
        "source-map-explorer": "^1.5.0",
        "ts-node": "^4.1.0",
        "tslint": "^5.9.1",
        "typescript": "^2.7.1",
        "zone.js": "^0.8.19"
    }
}
