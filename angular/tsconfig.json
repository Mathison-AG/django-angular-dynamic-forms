{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@mesemus/*": [
                "./dist/@mesemus/*"
            ]
        },
        "declaration": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": [
            "es2017",
            "dom"
        ],
        "module": "es2015",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noUnusedParameters": true,
        "noUnusedLocals": false,
        "removeComments": false,
        "sourceMap": true,
        "strictNullChecks": true,
        "target": "es5",
        "typeRoots": [],
        "types": []
    }
}
